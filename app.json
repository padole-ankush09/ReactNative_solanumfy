{
  "expo": {
    "name": "Solanumfy",
    "slug": "solanumfy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favpng_leaf-vegetable-green-clip-art.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A8766"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/leaf-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.solanumfy.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
